    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-2"></div>
        <div class="col-sm-8">
        </div>
        <div class="col-sm-2"></div>
      </div>
      <div class="col-sm-2"></div>
      <div class="col-sm-10 songs-container">
        <h3>PlayList</h3>
        <div id="latest-songs">
        </div>
      </div>
      <div class="col-sm-10" id="search-results">
        <%= render 'partials/search' %>
        <div id="results">
        </div>
      </div>
      <div class="col-sm-2"></div>
    </div>
  </div>
</div>
